      <geometry>
        <!-- Using unit sphere and rescale to 6.5 cm diameter of tennis ball. -->
        <!-- TODO(skleff): Figure out if the scaling is correct to give us a 6.5 cm sphere. -->
        <mesh filename="package://robot_properties_kuka/meshes/sphere.obj" scale="0.065 0.065 0.065"/>/>
      </geometry>
      <material name="grey">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
    </visual>

    <collision>
